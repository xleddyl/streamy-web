{
   "name": "template-nuxt",
   "version": "1.0.0",
   "private": true,
   "author": "xleddyl <edoalberti99@gmail.com>",
   "repository": "git@github.com:xleddyl/template-nuxt.git",
   "license": "MIT",
   "scripts": {
      "_env": "dotenv -e ./.env -c --",
      "_format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,html,json,sql,vue}\" --log-level error",
      "_format:clean": "prettier --write \"**/*.{ts,tsx,js,jsx,md,html,json,vue}\" --log-level error",
      "format": "yarn _format:clean --config ./.prettierrc-clean && yarn _format",
      "convert-webp": "cd ./public/images && bash ../../script/convert-webp.sh",
      "nuxt": " ",
      "build": "nuxt build",
      "dev": "nuxt dev --host",
      "preview": "nuxt build && nuxt preview",
      "aws": " ",
      "aws:sso": "aws sso login --sso-session xleddyl",
      "aws:sso-test": "aws sts get-caller-identity --profile hexcode",
      "aws:sso-config": "aws configure sso",
      "pulumi": " ",
      "pulumi:up": "yarn _env pulumi up",
      "pulumi:destroy": "yarn _env pulumi destroy"
   },
   "dependencies": {
      "@nuxt/scripts": "latest",
      "@nuxt/ui": "2.16.0",
      "@nuxtjs/device": "^3.1.1",
      "@nuxtjs/i18n": "^8.3.1",
      "@nuxtjs/seo": "latest",
      "@nuxtjs/sitemap": "latest",
      "@nuxtjs/supabase": "1.4.0",
      "@pinia/nuxt": "^0.5.1",
      "@supabase/ssr": "^0.5.1",
      "@tanstack/vue-query": "^5.37.1",
      "@vercel/analytics": "^1.4.1",
      "@vueuse/core": "^10.10.0",
      "@vueuse/nuxt": "^10.10.0",
      "dayjs-nuxt": "latest",
      "markdown-it": "^14.1.0",
      "motion": "^11.15.0",
      "nuxt": "3.12.2",
      "nuxt-zod-i18n": "latest",
      "pinia": "^2.1.7",
      "three": "^0.172.0",
      "vue": "^3.4.21",
      "vue-router": "^4.3.0",
      "vue-svg-loader": "^0.16.0"
   },
   "devDependencies": {
      "@pulumi/aws": "^6.0.0",
      "@pulumi/awsx": "^2.0.2",
